#include "main.h"

/**
* jack_bauer - Prints every minute of the day.
* Return: void
*/
void jack_bauer(void)
{
int h, h1, m, m1;

	for (h = 0, h < 3, h++)
	{
	_putchar(h);
	for (h1 = 0, h1 < 10, h1++)
	{	_putchar(h1);
		_putchar(':');
		for (m = 0, m < 10, m++)
		{
			_putchar(m);
			for (m1 = 0, m < 10, m++)
				_putchar(m1);
		}
	}
	}
	_putchar('\n');
}
